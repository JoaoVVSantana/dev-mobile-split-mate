{
  "expo": {
    "name": "splitMate",
    "slug": "splitmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/logo.png",
        "backgroundColor": "#5a139a"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/images/react-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#5a139a"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9a84c14a-792e-4cfd-bf13-2333a158ca85"
      }
    },
    "owner": "joaovvsantana"
  }
}
